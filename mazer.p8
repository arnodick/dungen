cartdata("mazer")

debug=false

function _init()
	state=0
	timer=0
	changestate(state)

	enums={}
	--actor types
	
	--game states
	enums.title=1
	enums.options=2
	enums.game=3
	
	hud={}
	hud.bar={}
	hud.bar.x=12
	hud.bar.y=10
	hud.bar.w=100
	hud.bar.h=6
	hud.bar.c=8
	hud.score={}
	hud.score.x=12
	hud.score.y=4
	hud.hp={}
	hud.hp.x=100
	hud.hp.y=4
end

function generatelandscape(gh,hh,gw,hw,hs,first)
end

function distance(x1,y1,x2,y2)
	--need to div by 10 bc numbers were getting way to big and overflowing, giving negatives output
	local a=(x2-x1)/10  local b=(y2-y1)/10
	return (a*a+b*b)
end

function makeactor(t,x,y)
	local actor={}
	actor.t=t
	actor.x=x
	actor.y=y
--	actor.vec={0,0}
	actor.delta=timer
	actor.step=0
	add(actors,actor)
	return actor
end

function makehitbox(a,x,y,w,h)
	a.hitbox={}
	a.hitbox.x=x
	a.hitbox.y=y
	a.hitbox.w=w
	a.hitbox.h=h
end

function drawactor(a)
	
end

function collision(a,enemy)
	if a.x>enemy.x+enemy.hitbox.x then
	if a.x<enemy.x+enemy.hitbox.x+enemy.hitbox.w then--+a.vec[1]*a.vel
	if a.y>enemy.y+enemy.hitbox.y then---a.vec[2]*a.vel
	if a.y<enemy.y+enemy.hitbox.y+enemy.hitbox.h then
		return true
	end
	end
	end
	end
	return false
end

function direction(d)
	local dire={0,0}
	if d==0 then dire[1]=-1 end
	if d==1 then dire[1]= 1 end
	if d==2 then dire[2]=-1 end
	if d==3 then dire[2]= 1 end
	return dire
end

function checkneighbours(ch,x,y)
	local cell=mget(x,y)
	if cell==0 or cell==17 then
		return 4
	elseif cell==18 then
		return -1
	end
	local adj=0
	local dire={}
	for a=0,3 do
		dire=direction(a)
		local cell=mget(x+dire[1],y+dire[2])
		if cell==ch
		or cell==17
		then
			adj+=1
--		elseif cell==18 then
--			return -1
		end
	end
	return adj
end

function checkstuck(ch,x,y)
	local routes=4
	for i=0,3 do
		local dire=direction(i)
		if checkneighbours(ch,x+dire[1],y+dire[2])>=2
		then
			routes-=1
		end
	end
	return routes
end

function controlactor(a)
	local vec={0,0}
--	a.vec[1]=0
--	a.vec[2]=0
	if a.t==1 then
		if     btn(0) then vec[1]=-1
		elseif btn(1) then vec[1]= 1
		elseif btn(2) then vec[2]=-1
		elseif btn(3) then vec[2]= 1
		end
	elseif a.t==2 then
		vec=direction(flr(rnd(4)))
	end
--	local cell=mget(a.x+vec[1],a.y+vec[2])
	local cn=checkneighbours(0,a.x+vec[1],a.y+vec[2])
	if cn==-1 then
--		mset(a.x,a.y,0)
--		speed=10
		changestate(state)
	elseif cn<2 then
--	and vec!=steps[a.step+1]
--	then
		a.x+=vec[1]
		a.y+=vec[2]
		mset(a.x,a.y,0)
		if mget(a.x,a.y-1)==1 then
			mset(a.x,a.y-1,2)
		end
		a.step+=1
		if a.step>#steps then
			add(steps,vec)
		else
			steps[a.step]=vec
		end
	elseif checkstuck(0,a.x,a.y)<=0
--	or vec==steps[a.step]
	then
		mset(a.x,a.y,17)
--		if mget(a.x,a.y-1)!=17 then
--			mset(a.x,a.y,19)
--		end
		a.step-=1
		if a.step<=0 then
--			if #steps>0 then
				changestate(state)
--			end
		else
			a.x-=steps[a.step][1]
			a.y-=steps[a.step][2]
		end
--		del(steps,steps[#steps])
		--changestate(state)
	end
end

function damageactor(a,d)

end

function spawnentities()

end

function _update()
	if timer%speed==0 then
		foreach(actors,controlactor)
	end
	timer+=1
	if debug then
		debug_u()
	end
end

function _draw()
	cls()
	camera(cam[1],cam[2])
	map(0,0,0,0,16,16)
	if debug then
		for a=1,#debug_l do
			print(debug_l[a],cam[1]+0,cam[2]+(a-1)*6,8)
		end
	end
end

function clamp(v,mi,ma,h)
	if h then
		if v<mi then v=mi
		elseif v>ma then v=ma
		end
	else
		if v<mi then v=ma
		elseif v>ma then v=mi
		end
	end
	return v
end

function rndint(n)
	return	flr(rnd(n))+1
end

function changestate(s)
	state=s
	timer=0
	speed=1
	steps={}
	debug_l={}
	cam={0,0}
	cam.shake=0
	
	actors={}
	introtext={}
	titletimer=0
	music(-1)
	reload()
	if state==0 then
		makeactor(2,rndint(16),rndint(16))
		mset(rndint(16)-1,rndint(16)-1,18)
	end
end

function debug_u()
	debug_l[1]=timer
	debug_l[2]="mem="..stat(0)
	debug_l[3]="cpu="..stat(1)
	debug_l[4]="actors:"..#actors
	debug_l[5]="step:"..actors[1].step
	for a=1,#steps do
		debug_l[a+5]=steps[a][1].." "..steps[a][2]
	end
end
